<UserControl x:Class="Solid2D.Editor.Controls.ProjectExplorerPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controlPanels="clr-namespace:Solid2D.Editor.Controls.ControlPanels"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary 
                  Source="../../Styles/DockablePanelStyle.xaml">
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
    </UserControl.Resources>
    <controlPanels:DockablePanel Caption="Project Explorer">
        <TextBlock>Project explorer</TextBlock>
    </controlPanels:DockablePanel>
</UserControl>
